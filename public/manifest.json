{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Loci's OW App",
    "author": "Arco Dielhof - Tex - AzerPUG",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.160.0",
    "description": "Loci's OW App",
    "dock_button_title": "Loci's OW App",
    "icon": "icons/IconMouseOver.png",
    "icon_gray": "icons/IconMouseNormal.png",
    "launcher_icon": "icons/desktop-icon.ico",
    "window_icon": "icons/IconMouseOver.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo",
    "FileSystem"
  ],
  "data":
  {
    "start_window": "background",
    "windows": {
      "background": {
        "file": "background.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "main": {
        "file": "main.html",
        "in_game_only": false,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "showhide",
        "resizable": false,
        "transparent": true,
        "override_on_update": true,
        "size": {"width": 500, "height": 700},
        "min_size": {"width": 500, "height": 700}
      }
    },
    "game_targeting":
    {
      "type": "dedicated",
      "game_ids":
      [
         5426,  5855,  6365,  7212,  7314,  7764,  8032,  9898, 10624, 10746,
        10798, 10826, 10844, 10906, 21216, 21308, 21404, 21566, 21586, 21626,
        21634, 21640, 21668, 21848, 21854, 21876, 21960
      ]
    },
    "game_events":
    [
      5426,  5855,  6365,  7212,  7314,  7764,  8032,  9898, 10624, 10746,
     10798, 10826, 10844, 10906, 21216, 21308, 21404, 21566, 21586, 21626,
     21634, 21640, 21668, 21848, 21854, 21876, 21960
   ],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data":
        {
          "game_ids":
          [
            5426,  5855,  6365,  7212,  7314,  7764,  8032,  9898, 10624, 10746,
           10798, 10826, 10844, 10906, 21216, 21308, 21404, 21566, 21586, 21626,
           21634, 21640, 21668, 21848, 21854, 21876, 21960
         ]
        },
        "start_minimized": false
      }
    ],
    "hotkeys":
    {
      "showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F"
      }
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": [
        "http://*.overwolf.com",
        "https://*.overwolf.com",
        "https://*.google-analytics.com",
        "http://*.google-analytics.com",
        "https://www.googleapis.com",
        "https://hooks.nabu.casa"
      ]
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
